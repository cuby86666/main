                        <div class="col-md-8">
                            <apex:inputField value="{!objCont.Community_web_country__c}" required="true" styleclass="btn btn-default dropdown-toggle input-form" id="Country" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-md-4" for="StateProvince">State/Province<span class="lighten">(optional)</span></label>
                        <div class="col-md-8">
                            <apex:inputField value="{!objCont.State_Province__c}"  styleclass="btn btn-default dropdown-toggle input-form" id="StateProvince" />
                        </div>
                    </div>
                    
                </div>
