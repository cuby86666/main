                        <div class="col-md-8">
                            <apex:inputField value="{!objCont.Company__c}" rendered="{!isInputCompany}" required="true" styleclass="btn btn-default dropdown-toggle input-form" id="inCompany" />
                        </div>
                    </div>
                    </apex:outputPanel>
