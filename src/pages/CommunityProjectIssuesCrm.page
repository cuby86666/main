                        <div class="col-md-2 col-xs-2"><span class="pull-left"><apex:outputText value="{0,date,MMMM dd, yyyy}">
                                                            <apex:param value="{!objIssue1.createdDate}" />
                                                       </apex:outputText></span>                               
                        </div>
                                                      
                    </div>
                    </apex:repeat>
                 
