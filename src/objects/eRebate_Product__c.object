    <validationRules>
        <fullName>CappedPayout_more_than_Estimated_Rebate</fullName>
        <active>true</active>
        <description>1508 Release. Capped Payout (USD) should be more than Estimated Rebate Amount</description>
        <errorConditionFormula>Capped_Payout__c &lt;  Estimated_Rebate_Amount__c</errorConditionFormula>
        <errorDisplayField>Capped_Payout__c</errorDisplayField>
        <errorMessage>Capped Payout should be more than Estimated Rebate Amount</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Rebate_Product_record_type_not_aligned</fullName>
        <active>true</active>
        <description>1508 Release. Rebate Product record type does not align with Rebate Program Type.</description>
        <errorConditionFormula>OR(
AND(
 eRebate__r.RecordType.Name = &quot;Distribution ODM Fullfillment&quot;,
 RecordType.Name &lt;&gt; &quot;Distribution ODM Fullfillment Rebate Product&quot;
),
AND(
 eRebate__r.RecordType.Name = &quot;Incentive / T&amp;Cs&quot;,
 RecordType.Name &lt;&gt; &quot;Incentive / T&amp;Cs / Contractual Rebate Product&quot;
),
AND(
 eRebate__r.RecordType.Name = &quot;OEM / EMS&quot;,
 RecordType.Name &lt;&gt; &quot;OEM / EMS Rebate Product&quot;
)
)</errorConditionFormula>
        <errorMessage>Rebate Product record type does not align with Rebate Program Type. Please click &quot;cancel&quot; and go back to the original Rebate to re-create Rebate Product(s).</errorMessage>
    </validationRules>
