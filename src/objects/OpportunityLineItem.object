    <validationRules>
        <fullName>CompeReqForCapturingStrengthWeakness</fullName>
        <active>true</active>
        <errorConditionFormula>AND  ( ISBLANK(TEXT(Competitor__c)), 
      (OR
       ( NOT
        (ISBLANK(CompetitorsStrengths__c )),
         NOT
        ( ISBLANK(Competitors_Weaknesses__c ))
       )
      )
     )</errorConditionFormula>
        <errorDisplayField>Competitor__c</errorDisplayField>
        <errorMessage>Competitor value is required to capture Competitor&apos;s Strengths and Weaknesses</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MN_Opportunity_Product_Not_Editable</fullName>
        <active>true</active>
        <description>Only Sales Price and Component Multiplier of Model N Opportunity Product cannot be edited by non-admin nor non-API users.</description>
        <errorConditionFormula>AND(
OR(
ISNEW(),
ISCHANGED( Description ),
ISCHANGED( Spin_Off__c ),
ISCHANGED( Count_Product_Revenue__c ),
ISCHANGED( Prod_Lost_Cancelled__c ),
ISCHANGED( Prod_Lost_Cancelled_Reason__c ),
ISCHANGED( Prod_Lost_Reason__c ),
ISCHANGED( Competitor__c ),
ISCHANGED( CompetitorsStrengths__c ),
ISCHANGED( Competitors_Weaknesses__c )
),
NOT(
OR(
$User.ProfileId=&quot;00e20000001Nc7g&quot;,
$User.ProfileId=&quot;00eD0000001oBDw&quot;
)
),
Opportunity.RecordType.DeveloperName = &quot;Model_N_Oppty&quot;
)</errorConditionFormula>
        <errorMessage>Only Sales Price and Component Multiplier of Model N Opportunity Product can be edited directly in Salesforce.com., the rest cannot be edited, as they are sync&apos;ed in from Model N.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Non_TMMA_MN_Oppty_Prod_not_editable</fullName>
        <active>true</active>
        <description>Non-TMMA MN oppty prod is not editable</description>
        <errorConditionFormula>AND( 
NOT(ISPICKVAL(Opportunity.Account.Customer_Category__c, &quot;Tier 4 - TMMA&quot;)),
NOT( 
OR( 
$User.ProfileId=&quot;00e20000001Nc7g&quot;, 
$User.ProfileId=&quot;00eD0000001oBDw&quot; 
) 
), 
Opportunity.RecordType.DeveloperName = &quot;Model_N_Oppty&quot; 
)</errorConditionFormula>
        <errorMessage>Non-TMMA MN oppty prod is not editable in Salesforce.com.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Oppty_Prod_Not_Editable</fullName>
        <active>true</active>
        <description>SFDC Opportunity Product cannot be edited after Commitment stage.</description>
        <errorConditionFormula>AND( 
ISPICKVAL( Opportunity.StageName ,&apos;Commitment&apos;)
, 
NOT( 
OR( 
$User.ProfileId=&quot;00e20000001Nc7g&quot;, 
$User.ProfileId=&quot;00eD0000001oBDw&quot; 
) 
) 
)</errorConditionFormula>
        <errorMessage>SFDC Opportunity Product cannot be edited after Commitment stage.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Qyt_Per_Sys_Integer</fullName>
        <active>true</active>
        <description>Qty per Sys should be integer and greater than 0.</description>
        <errorConditionFormula>AND( 
OR( 
VALUE( LEFT(TEXT(Component_Multiplier__c), FIND(&apos;.&apos;, TEXT(Component_Multiplier__c))) ) &gt; 0, 
VALUE( LEFT(TEXT(Component_Multiplier__c), FIND(&apos;.&apos;, TEXT(Component_Multiplier__c)) - 1) ) &lt;= 0, Component_Multiplier__c &lt;= 0 
), 
NOT(OR($User.ProfileId = &apos;00e20000001Nc7g&apos;, $User.ProfileId = &apos;00eD0000001oBDw&apos;)) 
)</errorConditionFormula>
        <errorDisplayField>Component_Multiplier__c</errorDisplayField>
        <errorMessage>Qty per Sys should be integer and greater than 0.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ReasonDescCompetitorForLossCancelled</fullName>
        <active>true</active>
        <errorConditionFormula>AND
(
 OR
   (
    ISPICKVAL(Prod_Lost_Cancelled__c,&quot;Lost&quot;),
    ISPICKVAL(Prod_Lost_Cancelled__c,&quot;Cancelled&quot;)
   ),
 OR
   (
   ISBLANK(TEXT(Prod_Lost_Cancelled_Reason__c)),
   ISBLANK(Prod_Lost_Reason__c),
   ISBLANK(TEXT(Competitor__c))
   ),
 Opportunity.RecordType.DeveloperName = &quot;SFDC_Oppty&quot;,
 NOT
 (
   OR($User.ProfileId=&quot;00e20000001Nc7g&quot;, 
      $User.ProfileId=&quot;00eD0000001oBDw&quot;) 
 )
)</errorConditionFormula>
        <errorMessage>Value for all the following three fields are required if the Opportunity Product is Lost or Cancelled. - Prod. Lost/Cancelled Reason, 	Prod. Lost/Cancelled Description, Competitor</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SFDC_Oppty_Prod_Not_Editable</fullName>
        <active>true</active>
        <description>SFDC Opportunity Product cannot be edited after Commitment stage.</description>
        <errorConditionFormula>AND( 
Opportunity.RecordType.DeveloperName = &quot;SFDC_Oppty&quot;, 
ISPICKVAL( Opportunity.StageName ,&apos;Commitment&apos;)
, 
NOT( 
OR( 
$User.ProfileId=&quot;00e20000001Nc7g&quot;, 
$User.ProfileId=&quot;00eD0000001oBDw&quot; 
) 
) 
)</errorConditionFormula>
        <errorMessage>SFDC Opportunity Product cannot be edited after Commitment stage.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Sales_Price_in_6_decimal_points</fullName>
        <active>true</active>
        <description>Sales_Price_in_6_decimal points per NXP F&amp;A rule</description>
        <errorConditionFormula>AND(
	Opportunity.RecordType.DeveloperName = &quot;SFDC_Oppty&quot;,
	LEN(TEXT(UnitPrice )) - FIND( &quot;.&quot;, TEXT(UnitPrice )) &gt;6,
	NOT( 
		OR( 
			$User.ProfileId=&quot;00e20000001Nc7g&quot;, 
			$User.ProfileId=&quot;00eD0000001oBDw&quot; 
		) 
	)
)</errorConditionFormula>
        <errorDisplayField>UnitPrice</errorDisplayField>
        <errorMessage>Unit Price in 6 decimal points</errorMessage>
    </validationRules>
