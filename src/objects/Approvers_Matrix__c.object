    <validationRules>
        <fullName>Approver_is_required</fullName>
        <active>true</active>
        <errorConditionFormula>( 
ISPICKVAL(Type__c , &apos;MAG Approver&apos;) || 
ISPICKVAL(Type__c, &apos;BL Approver&apos;) ||
ISPICKVAL(Type__c, &apos;GAM&apos;) || 
ISPICKVAL(Type__c, &apos;EVP&apos;) || 
ISPICKVAL(Type__c, &apos;Segment VP&apos;)
) &amp;&amp;
ISBLANK(Approver__c)</errorConditionFormula>
        <errorDisplayField>Approver__c</errorDisplayField>
        <errorMessage>Approver is a required field when Type is &apos;MAG Approver&apos; or &apos;BL Approver&apos; or &apos;GAM&apos; or &apos;EVP&apos; or &apos;Segment VP.&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_For_Signed_ARIA</fullName>
        <active>true</active>
        <errorConditionFormula>(
(ISPICKVAL(Type__c , &apos;MAG Approver&apos;) &amp;&amp; ISPICKVAL(Criteria_2__c , &apos;STAR&apos;)) ||
(ISPICKVAL(Type__c, &apos;BL Approver&apos;) &amp;&amp; ISPICKVAL(Criteria_2__c, &apos;STAR&apos;)) ||
(ISPICKVAL(Type__c , &apos;GAM&apos;) &amp;&amp; Criteria_1__c = &apos;STAR&apos;) ||  
(ISPICKVAL(Type__c , &apos;EVP&apos;) &amp;&amp; Criteria_1__c = &apos;STAR&apos;)
) &amp;&amp;
(Approver__r.Signed_ARIA__c = False)</errorConditionFormula>
        <errorDisplayField>Approver__c</errorDisplayField>
        <errorMessage>Signed ARIA should be checked for STAR Approvers.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Criteria1_Mandatory_For_Types</fullName>
        <active>true</active>
        <errorConditionFormula>(
ISPICKVAL(Type__c , &apos;MAG Approver&apos;) || 
ISPICKVAL(Type__c, &apos;BL Approver&apos;) ||
ISPICKVAL(Type__c, &apos;GAM&apos;) ||
ISPICKVAL(Type__c, &apos;EVP&apos;) ||
ISPICKVAL(Type__c, &apos;Segment VP&apos;) ||
ISPICKVAL(Type__c, &apos;GSM VP 2&apos;)
) &amp;&amp; 
ISBLANK(Criteria_1__c)</errorConditionFormula>
        <errorDisplayField>Criteria_1__c</errorDisplayField>
        <errorMessage>&apos;Criteria 1&apos;  is a required field when Type is &apos;MAG Approver&apos; or &apos;BL Approver&apos; or &apos;GAM&apos; or &apos;EVP&apos; or &apos;Segment VP&apos; or &apos;GSM VP 2.&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Criteria2_Mandatory_For_Sales_Director</fullName>
        <active>false</active>
        <description>Criteria 2(Region) is mandatory for sales director / Regional VP approver</description>
        <errorConditionFormula>(ISPICKVAL(Type__c , &apos;Sales Director Approver&apos; )|| ISPICKVAL( Type__c , &apos;Regional VP Approver&apos;)) &amp;&amp;  ISPICKVAL(Criteria_2__c , &apos;&apos; )</errorConditionFormula>
        <errorDisplayField>Criteria_2__c</errorDisplayField>
        <errorMessage>&apos;Criteria 2&apos; (Region) is a required field if Type is &apos;Sales Director&apos; OR &apos;Regional VP&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Criteria2_Mandatory_For_Types</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(Type__c, &apos;GSM VP 2&apos;) 
&amp;&amp; 
ISPICKVAL(Criteria_2__c , &apos;&apos;)</errorConditionFormula>
        <errorDisplayField>Criteria_2__c</errorDisplayField>
        <errorMessage>&apos;Criteria 2&apos; is a required field when Type is &apos;GSM VP 2.&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Threshold_Mandatory_MAG_STAR_Approvers</fullName>
        <active>false</active>
        <description>This field is created to make Mag Threshold Mandatory if type is &apos;MAG approver&apos; or &apos;MAG Approver-STAR&apos;</description>
        <errorConditionFormula>(ISPICKVAL( Type__c , &apos;MAG Approver&apos;)|| ISPICKVAL( Type__c , &apos;MAG Approver-STAR&apos;) ||  ISPICKVAL(Type__c, &apos;BL Approver&apos; )||
ISPICKVAL(Type__c, &apos;BL Approver-STAR&apos; ) ) &amp;&amp; ISNULL(Threshold_1__c)</errorConditionFormula>
        <errorDisplayField>Threshold_1__c</errorDisplayField>
        <errorMessage>Threshold is a required field if Type is &apos;MAG Approver&apos; or &apos;MAG Approver-STAR&apos; or &apos;BL Approver&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Type_is_required</fullName>
        <active>false</active>
