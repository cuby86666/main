    <validationRules>
        <fullName>Status_Reason_Mandatory</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(Status_Reason__c) &amp;&amp;
OR(ISPICKVAL(Project_Status__c , &apos;Red&apos;), ISPICKVAL( Project_Status__c , &apos;Yellow&apos;))</errorConditionFormula>
        <errorDisplayField>Status_Reason__c</errorDisplayField>
        <errorMessage>Status Reason is mandatory if Project Status is either &apos;Yellow&apos; or &apos;Red&apos;.</errorMessage>
    </validationRules>
