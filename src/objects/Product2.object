    <fields>
        <fullName>Alternative_BG__c</fullName>
        <description>Alternative BG is annually updated every Q4, and its source is GSM Controller.</description>
        <externalId>false</externalId>
        <formula>CASE(
    BU__c ,
    &apos;510&apos; , &apos;8000 High Performance Mixed Signal&apos; ,
    &apos;513&apos; , &apos;8000 High Performance Mixed Signal&apos; ,
    &apos;514&apos; , &apos;8000 High Performance Mixed Signal&apos; ,
    &apos;525&apos; , &apos;8000 High Performance Mixed Signal&apos; ,
    &apos;527&apos; , &apos;8000 High Performance Mixed Signal&apos; ,
    &apos;580&apos; , &apos;8000 High Performance Mixed Signal&apos; ,
    &apos;511&apos; , &apos;8001 Standard Products&apos; ,
    &apos;581&apos; , &apos;8001 Standard Products&apos; ,
    &apos;540&apos; , &apos;8020 Technology &amp; Operations&apos; ,
    &apos;541&apos; , &apos;8020 Technology &amp; Operations&apos; ,
    &apos;591&apos; , &apos;8025 Corporate &amp; Other&apos; ,
    &apos;592&apos; , &apos;8025 Corporate &amp; Other&apos; ,
    &apos;593&apos; , &apos;8025 Corporate &amp; Other&apos; ,
    &apos;594&apos; , &apos;8025 Corporate &amp; Other&apos; ,
    &apos;599&apos; , &apos;8025 Corporate &amp; Other&apos; ,
    &apos;&apos;
)</formula>
        <label>Alternative BG</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Alternative_BL__c</fullName>
        <description>Alternative BL is annualy updated every Q4, and its source is GSM Controller.</description>
        <externalId>false</externalId>
        <formula>IF(
    MAG__c = &apos;R67&apos; ,
    &apos;BL01 BL SECURE INTERFACE &amp; POWER&apos; ,
    BL__c &amp; &apos; &apos; &amp;  BL_Description__c
)</formula>
        <label>Alternative BL</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Alternative_BU__c</fullName>
