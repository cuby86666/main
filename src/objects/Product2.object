        <externalId>false</externalId>
        <formula>IF( 
    MAG__c = &apos;R67&apos; ,
    &apos;510 BU SECURITY &amp; CONNECTIVITY&apos; ,
    IF(
        BU__c = &apos;513&apos; ,
        &apos;511 BU STANDARD PRODUCTS&apos; ,
        BU__c &amp; &apos; &apos; &amp;  BU_Description__c 
